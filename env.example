# VORTENA API Configuration
# Copy this file to .env and fill in your actual values

# Your Solana wallet private key (Base58 encoded)
PRIVATE_KEY=your_base58_private_key_here

# Token mint address (contract address) you want to trade
TOKEN_MINT_ADDRESS=your_token_mint_address_here

# Trading Configuration (Optional - defaults will be used if not set)
TRADE_AMOUNT=0.01
SELL_PERCENTAGE=50
SLIPPAGE_TOLERANCE=5
PRIORITY_FEE=0.003

# API Configuration (Optional)
VORTENA_API_BASE_URL=https://api.vortena.xyz
API_TIMEOUT=30

# Development Settings
NODE_ENV=development
DEBUG=false 